{
  "name": "strider-node",
  "version": "0.3.2",
  "description": "Run Nodejs tests in Strider",
  "main": "worker.js",
  "scripts": {
    "test": "jshint && mocha -R tap"
  },
  "dependencies": {
    "strider-detection-rules": "0.0.1",
    "npm": "~1.3.11",
    "n": "~1.1.0",
    "MD5": "~1.1.0",
    "async": "~0.2.9",
    "mkdirp": "~0.3.5"
  },
  "keywords": [
    "strider",
    "node"
  ],
  "author": "Peter Braden",
  "license": "BSD",
  "strider": {
    "id": "node",
    "type": "job",
    "title": "Node",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": {
      "controller": "NodeController"
    },
    "build-status": {
      "style": "config/build.less",
      "template": "config/build.html",
      "attrs": {
        "ng-show": "data.doTest",
        "class": "[[ data.testCmd ]]"
      }
    }
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.14.0",
    "jshint": "~2.3.0"
  }
}
