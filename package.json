{
  "name": "strider-node",
  "version": "1.0.1",
  "description": "Run Nodejs tests in Strider",
  "main": "worker.js",
  "scripts": {
    "test": "eslint webapp.js worker.js lib/ && node test | tap-spec",
    "postinstall": "npm install n npm || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Strider-CD/strider-node"
  },
  "keywords": [
    "strider",
    "node"
  ],
  "author": "Peter Braden",
  "license": "BSD-3-Clause",
  "dependencies": {
    "md5": "^2.0.0",
    "async": "^1.0.0",
    "fs-extra": "^0.8.1"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "tap-spec": "^1.0.1",
    "tape": "^3.0.3"
  },
  "strider": {
    "id": "node",
    "type": "job",
    "title": "Node",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": {
      "controller": "NodeController"
    },
    "build-status": {
      "style": "config/build.less",
      "template": "config/build.html",
      "attrs": {
        "ng-show": "data.doTest",
        "class": "[[ data.testCmd ]]"
      }
    }
  }
}
