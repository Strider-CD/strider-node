{
  "name": "strider-node",
  "version": "0.2.4",
  "description": "Run Nodejs tests in Strider",
  "main": "worker.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "strider-detection-rules": "0.0.1",
    "npm": "~1.3.11",
    "n": "~1.1.0"
  },
  "keywords": [
    "strider",
    "node"
  ],
  "author": "Peter Braden",
  "license": "BSD",
  "strider": {
    "id": "node",
    "type": "job",
    "title": "Node",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": true,
    "build-status": {
      "style": "config/build.less",
      "template": "config/build.html",
      "attrs": {
        "ng-show": "data.doTest",
        "class": "[[ data.testCmd ]]"
      }
    }
  }
}
